<!DOCTYPE html>
<html>
<head>
	<title><%= Title%></title>
	<link rel="stylesheet" href="/public/css/w3.css">
	<link rel="stylesheet" href="/public/css/my_member.css">
    <link rel="stylesheet" href="/public/css/toastr.min.css">
    <script src="/public/js/jquery.js"></script>
    <script src="/public/js/jquery.validate.js"></script>
    <script src="/public/js/toastr.min.js"></script>
    <script>
        jQuery.browser={};(function(){jQuery.browser.msie=false; jQuery.browser.version=0;if(navigator.userAgent.match(/MSIE ([0-9]+)./)){ jQuery.browser.msie=true;jQuery.browser.version=RegExp.$1;}})();
    </script>
	<% CSSs.forEach(function(CSS){ %>
		<link rel="stylesheet" href= <%= CSS%> >
	<%});%>
	<% JavaScripts.forEach(function(JavaScript){ %>
		<script src=<%= JavaScript%>></script>
	<%});%>
</head>
<body>
	<div id="wrap_top">
			<div id="logopic">
				<a href="/front"><img src="/public/images/logo.png"/></a>
			</div>
			<div class="wrap_top_hr"></div>
			<p class="wrap_top_p">會員中心</p>
			<div id="member"><span id="user"><a>Jacky Lee</a></span><span id="logout"><a>登出</a></span></div>
			<div class="clear"></div>
	</div>
    <div id="wrap_top_hide"></div>
	<div id="wrap_body">
		<div id="wrap_left">
			<% if(UserData.rank>=1){ %>
				<li><a href="">訂單管理</a></li><br/>
			<% } %>
			<% if(UserData.rank>=2){ %>
				<li><a href="/member/commodity">商品管理</a></li><br/>
				<li><a href="/member/news">新聞管理</a></li><br/>
			<% } %>
			<% if(UserData.rank==0){ %>
				<li><a href="">購買紀錄</a></li><br/>
				<li><a href="">最愛商品</a></li><br/>
			<% } %>
			<li><a href="">站內信</a></li><br/>
			<li><a href="">商品Q&A</a></li><br/>
			<li><a href="">個人資料</a></li><br/>
			<div class="clear"></div>
		</div>
		<div class="wrap_body_hr"></div>
		<div id="wrap_right">
			<% Include.forEach(function(IObject){%>
				<%- include(IObject.url,{value:IObject.value})%>
			<%});%>
		</div>
		<div class="clear"></div>
	</div>
	<div id="wrap_footer">
	</div>
	<script src="/public/js/member.js"></script>
</body>
	<% Script.forEach(function(SObject){%>
		<%- include(SObject.url,{value:SObject.value})%>
	<%});%>
</html>